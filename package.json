{
  "name": "@tendrel/checklist",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "android:clean": "bun recompile && bun android",
    "compile": "tsc",
    "precompile": "bun generate",
    "check": "biome check . --write --unsafe --error-on-warnings && relay-compiler --validate",
    "ci:check": "biome ci . --error-on-warnings && relay-compiler --validate",
    "ci:compile": "bun compile",
    "ci:doctor": "bunx expo-doctor",
    "ci:test": "jest",
    "clean": "rimraf .expo android ios node_modules",
    "eas-build-post-install": "relay-compiler",
    "generate": "drizzle-kit generate && relay-compiler",
    "ios": "expo run:ios",
    "ios:clean": "bun recompile && bun ios",
    "recompile": "bun clean && bun i && bun compile",
    "start": "expo start",
    "test": "jest --watch",
    "translate": "bun scripts/translate.ts && bun check",
    "web": "expo start --web",
    "android:stop-daemon": "cd android && ./gradlew --stop",
    "maestro:all": "maestro test test/flows",
    "relay": "relay-compiler"
  },
  "dependencies": {
