{
  "cli": {
    "version": ">= 7.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "autoIncrement": true,
      "channel": "base"
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "test": {
      "extends": "base",
      "channel": "test",
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NODE_ENV": "test"
      }
    },
    "production": {
      "extends": "base",
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
